<% if flash[:errors] %>
<% flash[:errors].each do |error| %>
<p><%= error %></p>
<% end %>
<% end %>


<%= form_for @vs do |f| %>

<%= f.label :vendor_id %>
<%= f.collection_select  :vendor_id, Vendor.all, :id, :name %>

<%= f.label :sweet_id %>
<%= f.collection_select  :sweet_id, Sweet.all, :id, :name %>

<%= f.label :price %>
<%= f.number_field :price%>
    
<%= f.submit %>

<% end %>